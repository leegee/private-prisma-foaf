datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model person {
  id            Int             @id @default(autoincrement())
  fullname      String
  published     Boolean         @default(false)
  person2person person2person[]
}

model person2person {
  id     Int    @id @default(autoincrement())
  person person @relation(fields: [id], references: [id])
}
