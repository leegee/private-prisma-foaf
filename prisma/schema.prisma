datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Person {
  id        Int      @id @default(autoincrement())
  fullname  String
  published Boolean  @default(false)
  friend    Person[] @relation("Person2Person", references: [id])
  friends   Person[] @relation("Person2Person", references: [id])
}
