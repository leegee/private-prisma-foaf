datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Person {
  id            Int             @id @default(autoincrement())
  fullname      String
  published     Boolean         @default(false)
  person2person Person2Person[]
}

model Person2Person {
  id     Int    @id @default(autoincrement())
  person Person @relation(fields: [id], references: [id])
}
